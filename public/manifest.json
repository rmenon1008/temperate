{
  "name": "Temperate",
  "description": "A  new-tab page with temperature and weather forecasts",
  "version": "0.2.0.0",
  "manifest_version": 3,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "options_page": "index.html?options",
  "action": {
    "default_popup": "index.html?popup"
  },
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}